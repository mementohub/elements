{
    "name": "imemento/elements",
    "description": "Essential components for iMemento Laravel services",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bogdan Cismariu",
            "email": "bogdan.cismariu@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "spatie/laravel-cors": "^1.3",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "imemento/activity-log": "^5.7",
        "imemento/clients": "dev-master",
        "imemento/exceptions": "^5.7",
        "imemento/exceptions-laravel": "^5.7",
        "imemento/guard-laravel": "^5.7",
        "imemento/health-probe": "^5.7",
        "imemento/http": "^5.7",
        "imemento/http-responses": "^5.7",
        "imemento/jwt": "^5.7",
        "imemento/sdk-auth": "^5.7",
        "imemento/support": "^5.7",
        "larafun/suite": "dev-master",
        "laravel/framework": "^5.7",
        "laravel/tinker": "^1.0",
        "ytake/laravel-fluent-logger": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench": "^3.7.0"
    },
    "autoload": {
        "psr-4": {
            "iMemento\\Elements\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "iMemento\\Elements\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "iMemento\\Elements\\ServiceProvider"
            ]
        }
    }
}
