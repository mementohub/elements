{
    "name": "imemento/elements",
    "description": "Essential components for iMemento Laravel services",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bogdan Cismariu",
            "email": "bogdan.cismariu@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "imemento/activity-log": "~6.0.0",
        "imemento/clients": "~6.0.0",
        "imemento/exceptions": "~6.0.0",
        "imemento/exceptions-laravel": "~6.0.0",
        "imemento/guard-laravel": "~6.0.0",
        "imemento/health-probe": "~6.0.0",
        "imemento/http": "~6.0.0",
        "imemento/http-responses": "~6.0.0",
        "imemento/jwt": "~6.0.0",
        "imemento/sdk-auth": "~6.0.0",
        "imemento/support": "~6.0.0",
        "larafun/suite": "~6.0.0",
        "ytake/laravel-fluent-logger": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "iMemento\\Elements\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "iMemento\\Elements\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "iMemento\\Elements\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
