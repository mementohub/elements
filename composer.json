{
    "name": "imemento/elements",
    "description": "Essential components for iMemento Laravel services",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bogdan Cismariu",
            "email": "bogdan.cismariu@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "spatie/laravel-cors": "*",
        "fideloper/proxy": "*",
        "guzzlehttp/guzzle": "*",
        "imemento/activity-log": "dev-master",
        "imemento/clients": "dev-master",
        "imemento/exceptions": "dev-master",
        "imemento/exceptions-laravel": "dev-master",
        "imemento/guard-laravel": "dev-master",
        "imemento/health-probe": "dev-master",
        "imemento/http": "dev-master",
        "imemento/http-responses": "dev-master",
        "imemento/jwt": "dev-master",
        "imemento/sdk-auth": "dev-master",
        "imemento/support": "dev-master",
        "larafun/suite": "dev-master",
        "ytake/laravel-fluent-logger": ">=3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench": "^3.7.0"
    },
    "autoload": {
        "psr-4": {
            "iMemento\\Elements\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "iMemento\\Elements\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "iMemento\\Elements\\ServiceProvider"
            ]
        }
    }
}
